
<h1> My Pictures </h1>


<% @pics.each do |pic| %>
	<div class ="span5 stolen-surfbox text-center">
		<h2 class="custom-font2">
			Hours <%= pic.hour %>
		</h2>

		<div> 
			<%= image_tag pic.image %>

		</div>

		<h3>
			I feel <%= pic.emotion %>

		</h3>

		<p> 
			I learned that <%= pic.learned %>

</div>
<% end %>